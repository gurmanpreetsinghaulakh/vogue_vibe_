  <!-- GSAP Animation -->
  <script>
    gsap.registerPlugin(ScrollTrigger);

    // GSAP Animations
    gsap.from("#index-section .text-overlay", {
      scrollTrigger: {
        trigger: "#index-section",
        start: "top center",
        toggleActions: "play reverse play reverse",
      },
      opacity: 0,
      x: 50,
      duration: 1,
    });

    gsap.from("#product-types-section .product-list", {
      scrollTrigger: {
        trigger: "#product-types-section",
        start: "top center",
        toggleActions: "play reverse play reverse",
      },
      opacity: 0,
      y: 50,
      duration: 1,
    });

    gsap.from("#featured-products-section .product-list", {
      scrollTrigger: {
        trigger: "#featured-products-section",
        start: "top center",
        toggleActions: "play reverse play reverse",
      },
      opacity: 0,
      y: 50,
      duration: 1,
    });

    // Search Bar Toggle
    document.addEventListener("DOMContentLoaded", function () {
      const searchIcon = document.querySelector(".search-icon");
      const searchBar = document.querySelector(".search-bar");

      searchIcon.addEventListener("click", () => {
        searchBar.classList.toggle("open");
        if (searchBar.classList.contains("open")) {
          searchBar.querySelector("input").focus();
        }
      });
    });
  </script>
