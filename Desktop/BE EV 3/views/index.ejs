    /* Navbar Styles */
    .navbar {
      position: sticky;
      top: 0;
      background-color: #ffffff;
      z-index: 1000;
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 10px 50px;
      font-size: 1.2rem;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }
    .navbar-brand {
      font-weight: bold;
      font-size: 1.8rem;
    }
    .navbar-center {
      flex: 1;
      display: flex;
      justify-content: center;
      gap: 40px;
    }
    .navbar-center .nav-link {
      font-size: 1.2rem;
      text-decoration: none;
      color: black;
      padding: 5px 15px;
      transition: background-color 0.3s ease, color 0.3s ease;
    }
    .navbar-center .nav-link:hover {
      background-color: #f5f5f5;
      color: #333;
      border-radius: 5px;
    }
    .navbar-icons {
      display: flex;
      align-items: center;
      gap: 15px;
    }
